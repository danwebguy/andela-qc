{
  "name": "andela-qc",
  "version": "1.0.0",
  "description": "Crowd-source questions for a meetup",
  "main": "server.js",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "airbnb-browser-shims": "^3.1.0",
    "babel-istanbul": "^0.12.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "compression": "^1.7.3",
    "config": "^3.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "pg": "^7.8.0",
    "request": "^2.88.0",
    "superagent": "^4.1.0",
    "supertest": "^3.3.0",
    "uuid": "^3.3.2",
    "validator": "^10.10.0"
  },
  "scripts": {
    "postinstall": "npm run build:prod",
    "start": "node dist/server.js",
    "clean:dist": "rimraf ./dist",
    "build:prod": "npm run clean:dist && npm run prod-migrate && babel server.js --out-dir dist",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node server.js",
    "prod-migrate": "npm run migrate --env production",
    "test": "./node_modules/.bin/cross-env NODE_ENV=test && nyc mocha --timeout 15000 server/test/**/*.js --exit --compilers @babel/register ./server/test/**/*.js",
    "pretest": "eslint --ignore-path .gitignore .",
    "lint": "eslint --ignore-path .gitignore .",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov --require babel-core/register  _mocha --exit ../test/*.js",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate-report": "codeclimate-test-reporter < ./coverage/lcov.info",
    "migrate": "babel-node server/models/db.js"
  },
  "engines": {
    "node": "10.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danwebguy/andela-qc.git"
  },
  "author": "Daniel Ufeli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danwebguy/andela-qc/issues"
  },
  "homepage": "https://github.com/danwebguy/andela-qc#readme"
}
